    "rollup-plugin-visualizer": "^5.8.3",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.0.0",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^4.0.0",
    "vite-plugin-lint": "^1.0.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^4.0.0",
