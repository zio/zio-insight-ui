    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.8.3",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-icons": "^4.6.0",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^6.4.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
